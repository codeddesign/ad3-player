{
    "scripts": {
        "rollup": "npm run rollup-init & npm run rollup-player",
        "rollup-dev": "npm run rollup-init -- --environment build:dev -w & npm run rollup-player -- --environment build:dev -w",
        "rollup-init": "./node_modules/.bin/rollup -c --input src/init/base.js --output dist/assets/js/init.js",
        "rollup-player": "./node_modules/.bin/rollup -c --input src/player/base.js --output dist/assets/js/player.js",
        "test": "./node_modules/.bin/mocha --compilers js:babel-core/register",
        "test-watch": "npm run test -- --watch",
        "test-all": "./node_modules/.bin/mocha --compilers js:babel-core/register test/**/test*.js",
        "test-all-watch": "npm run test-all -- --watch"
    },
    "devDependencies": {
        "babel-loader": "^6.2.5",
        "babel-preset-es2015": "^6.16.0",
        "babel-preset-es2015-rollup": "^1.2.0",
        "chai": "^3.5.0",
        "mocha": "^3.1.2",
        "rollup": "^0.36.3",
        "rollup-plugin-babel": "^2.6.1",
        "rollup-plugin-commonjs": "^5.0.5",
        "rollup-plugin-node-resolve": "^2.0.0",
        "rollup-plugin-replace": "^1.1.1",
        "rollup-plugin-uglify": "^1.0.1",
        "rollup-watch": "^2.5.0"
    },
    "dependencies": {}
}
